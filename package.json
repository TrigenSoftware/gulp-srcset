{
  "name": "gulp-srcset",
  "version": "1.0.2",
  "description": "Highly customizable plugin to generating responsive images.",
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "eslint": "eslint --cache 'src/*.js'",
    "compile": "babel ./src -d ./lib",
    "prepublish": "npm run compile"
  },
  "repository": "https://github.com/TrigenSoftware/gulp-srcset.git",
  "keywords": [
    "gulp",
    "gulpplugin",
    "srcset",
    "image",
    "picture",
    "responsive"
  ],
  "author": "dangreen",
  "license": "MIT",
  "dependencies": {
    "css-mediaquery": "^0.1.2",
    "imagemin": "^5.2.2",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-svgo": "^5.2.0",
    "imagemin-webp": "^4.0.0",
    "imagemin-zopfli": "^5.0.0",
    "minimatch": "^3.0.3",
    "sharp": "^0.17.1",
    "vinyl": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-trigen": "^1.1.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
