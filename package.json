{
  "name": "gulp-srcset",
  "version": "2.0.0",
  "description": "Highly customizable plugin to generating responsive images.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrigenSoftware/gulp-srcset.git"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/gulp-srcset/issues"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "checkDeps": "npm-check -s; exit 0",
    "lint": "eslint --cache 'src/*.js'",
    "test": "npm run checkDeps && npm run lint",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "srcset",
    "image",
    "picture",
    "responsive"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.49",
    "@flexis/srcset": "^1.0.1",
    "through2-concurrent": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.52",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-stage-0": "^7.0.0-beta.49",
    "eslint": "^5.0.1",
    "eslint-config-trigen": "^3.2.0",
    "husky": "^1.0.0-rc.10",
    "npm-check": "^5.7.1",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^4.0.0-beta.5",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "files": [
    "lib"
  ]
}
